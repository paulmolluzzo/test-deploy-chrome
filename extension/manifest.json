{
  "manifest_version": 2,

  "name": "Test Deploy Chrome",
  "short_name": "Test Deploy Chrome",
  "description": "Tsting auto deployment to Chrome",
  "version": "0.0.1",

  "permissions": [
      "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
